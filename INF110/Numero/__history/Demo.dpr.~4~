program Demo;
uses
  System.SysUtils,
  Numero in 'Numero.pas';

var
  numero : cardinal;
begin
  write('Ingrese un numero: ');
  readln(numero);      //74287365
  writeln('el digito mayor es: ' , DigitoMayorDe4Dig(numero));
  writeln('la cantidad de digitos es: ' , CantidadDeDigitos(numero));

  readln;
end.
